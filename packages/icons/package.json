{
  "name": "@aemiko/icons",
  "version": "0.1.3",
  "repository": "git@github.com:amikos-software/verde.git",
  "author": "luserazo <serazo.luis@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "prebuild": "rimraf src && rimraf dist && yarn run svgr",
    "build": "microbundle --jsx React.createElement --format modern,cjs",
    "svgr": "svgr --icon --typescript --ref -d src -- assets",
    "prepare": "run-s build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "microbundle": "^0.13.3",
    "rimraf": "^3.0.2",
    "run-s": "^0.0.0",
    "typescript": "^4.7.4"
  }
}
